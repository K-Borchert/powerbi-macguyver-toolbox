          {
            "name": "Rolling 2-Week Sum",
            "expression": [
              "",
              "VAR _RollingSum = ",
              "    CALCULATE(",
              "        [Sum],",
              "        DATESINPERIOD(",
              "            'Date'[Date],",
              "            LASTDATE('Date'[Date]),",
              "            -14,",
              "            DAY",
              "        )",
              "    )",
              "VAR _MaxDateTable = MAX ( 'Date'[Date] )",
              "VAR _MaxDate = MAX ( 'Table'[Month] )",
              "VAR _Result = ",
              "    IF (_MaxDateTable > _MaxDate, _RollingSum )",
              "RETURN",
              "    _Result"
            ],
            "lineageTag": "2a3d3b47-00b8-44e3-9f64-d00a9b2f77bb"
